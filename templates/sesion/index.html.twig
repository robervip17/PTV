<!DOCTYPE html>

<title>Sesion index</title>

{% block body %}
    <h1>Sesion index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>FechaSesion</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for sesion in sesions %}
            <tr>
                <td>{{ sesion.id }}</td>
                <td>{{ sesion.fechaSesion ? sesion.fechaSesion|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('sesion_show', {'id': sesion.id}) }}">show</a>
                    <a href="{{ path('sesion_edit', {'id': sesion.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('sesion_new') }}">Create new</a>
{% endblock %}
