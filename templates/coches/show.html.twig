{% extends "dashboard/layout.html.twig" %}
{% block contenido %}
    <h1>Coches</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ coch.id }}</td>
            </tr>
            <tr>
                <th>Marca</th>
                <td>{{ coch.marca }}</td>
            </tr>
            <tr>
                <th>Modelo</th>
                <td>{{ coch.modelo }}</td>
            </tr>
            <tr>
                <th>Observaciones</th>
                <td>{{ coch.observaciones }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('coches_index') }}" class="btn btn-dark" style="margin-left:3%;">Volver a la lista</a>

    <a href="{{ path('coches_edit', {'id': coch.id}) }}" class="btn btn-dark">Editar</a>

    {{ include('coches/_delete_form.html.twig') }}
{% endblock %}
